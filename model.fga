model
  schema 1.1

type user

type house
  relations
    define member: [user] or admin or friend or partner
    define admin: [user]
    define friend: [user]
    define partner: [user]
    define guest: [user]
    define can_invite: partner


type app
  relations
    define homelab: [house]
    define all_access: admin from homelab
    define can_view: member from homelab 
